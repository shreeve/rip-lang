        "2": { "name": "keyword.control.inheritance.rip" },
        "3": { "name": "entity.other.inherited-class.rip" }
      },
      "name": "meta.class.rip"
    },
    {
      "match": "\\b(?<!\\.|::)(true|on|yes)(?!\\s*[:=][^=])\\b",
      "name": "constant.language.boolean.true.rip"
    },
    {
      "match": "\\b(?<!\\.|::)(false|off|no)(?!\\s*[:=][^=])\\b",
      "name": "constant.language.boolean.false.rip"
    },
    {
      "match": "\\b(?<!\\.|::)null(?!\\s*[:=][^=])\\b",
      "name": "constant.language.null.rip"
    },
    {
      "match": "\\b(?<!\\.|::)undefined(?!\\s*[:=][^=])\\b",
      "name": "constant.language.undefined.rip"
    },
    {
      "match": "\\b(?<!\\.|::)(NaN|Infinity)(?!\\s*[:=][^=])\\b",
      "name": "constant.language.rip"
    },
    {
      "match": "\\b(?<!\\.|::)this(?!\\s*[:=][^=])\\b",
      "name": "variable.language.this.rip"
    },
    {
      "captures": { "1": { "name": "variable.other.readwrite.instance.rip" } },
      "match": "(?<!\\w)(@)([a-zA-Z_$]\\w*)?",
      "name": "variable.other.readwrite.instance.rip"
    },
    {
      "match": "\\b(?<!\\.|::)(class|extends|return|throw|break|continue|switch|when|then|if|else|unless|for|in|of|by|as|while|until|loop|do|try|catch|finally|yield|await|import|export|from|default|delete|typeof|instanceof|new|super|debugger|use|own|not|and|or|is|isnt)(?!\\s*[:=][^=])\\b",
      "name": "keyword.control.rip"
    },
    {
      "match": "::=",
      "name": "keyword.operator.type-alias.rip"
    },
    {
      "match": "::(?!=)",
      "name": "keyword.operator.type-annotation.rip"
    },
    {
      "match": "~=|:=|~>|<=>",
      "name": "keyword.operator.assignment.reactive.rip"
    },
    {
      "match": "=!",
      "name": "keyword.operator.assignment.readonly.rip"
    },
    {
      "match": "!\\?",
      "name": "keyword.operator.otherwise.rip"
    },
    {
      "match": "=~",
      "name": "keyword.operator.regex-match.rip"
    },
    {
      "match": "\\?\\?=",
      "name": "keyword.operator.assignment.compound.rip"
    },
    {
      "match": "\\?\\?",
      "name": "keyword.operator.nullish.rip"
    },
    {
      "match": "\\?\\.(?![0-9])",
      "name": "keyword.operator.optional-chaining.rip"
    },
    {
      "match": "(?<=[a-zA-Z0-9_$)])!(?!=)",
      "name": "keyword.operator.dammit.rip"
    },
    {
      "match": "([a-zA-Z$_][\\w$]*)?\\s*(=|:(?!:))(?![>=])",
      "captures": {
        "1": { "name": "variable.assignment.rip" },
        "2": { "name": "keyword.operator.assignment.rip" }
      }
    },
    {
      "match": "&&=|\\|\\|=|\\?\\?=|\\+=|-=|\\*=|/=|%=|\\*\\*=|<<=|>>=|>>>=|&=|\\|=|\\^=",
      "name": "keyword.operator.assignment.compound.rip"
    },
    {
      "match": "===|!==|==|!=|<=|>=|<=>",
      "name": "keyword.operator.comparison.rip"
    },
    {
      "match": "&&|!|\\|\\|",
      "name": "keyword.operator.logical.rip"
    },
    {
      "match": "&|\\||\\^|~",
      "name": "keyword.operator.bitwise.rip"
    },
    {
      "match": "<<|>>>|>>",
      "name": "keyword.operator.bitwise.shift.rip"
    },
    {
      "match": "--",
      "name": "keyword.operator.decrement.rip"
    },
    {
      "match": "\\+\\+",
      "name": "keyword.operator.increment.rip"
    },
    {
      "match": "\\.\\.\\.",
      "name": "keyword.operator.splat.rip"
    },
    {
      "match": "\\.\\.",
      "name": "keyword.operator.range.rip"
    },
    {
      "match": "\\?",
      "name": "keyword.operator.existential.rip"
    },
    {
      "match": "\\*\\*|//|%%",
      "name": "keyword.operator.arithmetic.special.rip"
    },
    {
      "match": "%|\\*|/|-|\\+",
      "name": "keyword.operator.rip"
    },
    {
      "match": "\\b0x[0-9a-fA-F](?:_?[0-9a-fA-F])*n?\\b",
      "name": "constant.numeric.hex.rip"
    },
    {
      "match": "\\b0o[0-7](?:_?[0-7])*n?\\b",
      "name": "constant.numeric.octal.rip"
    },
    {
      "match": "\\b0b[01](?:_?[01])*n?\\b",
      "name": "constant.numeric.binary.rip"
    },
    {
      "match": "\\b[0-9][0-9_]*(?:\\.[0-9][0-9_]*)?(?:[eE][+-]?[0-9]+)?n?\\b",
      "name": "constant.numeric.decimal.rip"
    },
    {
      "begin": "'",
      "beginCaptures": { "0": { "name": "punctuation.definition.string.begin.rip" } },
      "end": "'",
      "endCaptures": { "0": { "name": "punctuation.definition.string.end.rip" } },
      "name": "string.quoted.single.rip",
      "patterns": [
        {
          "captures": { "1": { "name": "punctuation.definition.escape.backslash.rip" } },
          "match": "(\\\\).",
          "name": "constant.character.escape.backslash.rip"
        }
      ]
    },
    {
      "begin": "\"",
      "beginCaptures": { "0": { "name": "punctuation.definition.string.begin.rip" } },
      "end": "\"",
      "endCaptures": { "0": { "name": "punctuation.definition.string.end.rip" } },
      "name": "string.quoted.double.rip",
      "patterns": [
        {
          "captures": { "1": { "name": "punctuation.definition.escape.backslash.rip" } },
          "match": "(\\\\).",
          "name": "constant.character.escape.backslash.rip"
        },
        { "include": "#interpolated_rip" }
      ]
    }
  ],
  "repository": {
    "interpolated_rip": {
      "patterns": [
        {
          "begin": "\\#\\{",
          "beginCaptures": { "0": { "name": "punctuation.section.embedded.begin.rip" } },
          "end": "\\}",
          "endCaptures": { "0": { "name": "punctuation.section.embedded.end.rip" } },
          "name": "source.rip.embedded.source",
          "patterns": [{ "include": "$self" }]
        },
        {
          "begin": "\\$\\{",
          "beginCaptures": { "0": { "name": "punctuation.section.embedded.begin.rip" } },
          "end": "\\}",
          "endCaptures": { "0": { "name": "punctuation.section.embedded.end.rip" } },
          "name": "source.rip.embedded.source",
          "patterns": [{ "include": "$self" }]
        }
      ]
    },
    "function_params": {
      "patterns": [
        {
          "begin": "\\(",
          "beginCaptures": { "0": { "name": "punctuation.definition.parameters.begin.bracket.round.rip" } },
          "end": "\\)",
          "endCaptures": { "0": { "name": "punctuation.definition.parameters.end.bracket.round.rip" } },
          "name": "meta.parameters.rip",
          "patterns": [
            {
              "match": "([a-zA-Z_$][\\w$]*)(\\.\\.\\.)?",
              "captures": {
                "1": { "name": "variable.parameter.function.rip" },
                "2": { "name": "keyword.operator.splat.rip" }
              }
            },
            { "include": "#interpolated_rip" },
            { "include": "$self" }
          ]
        }
      ]
    }
  }
