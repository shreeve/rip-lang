# Home page â€” redirects to bookings or auth

export Home = component

  beforeMount: ->
    try
      res = await fetch('/api/user')
      data = await res.json()
      if data.user
        @router.push '/bookings'
      else
        @router.push '/auth'
    catch
      @router.push '/auth'

  render
    .page-center
      p "Loading..."
